{
  "name": "r2",
  "version": "2.4.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prestart": "npm run build",
    "start": "node ./dist | node ./dist/transport",
    "test": "npm run cov && cat ./coverage/lcov.info | coveralls",
    "posttest": "npm run lint",
    "jest": "jest --runInBand",
    "cov": "jest --runInBand --coverage",
    "lint": "tslint -p .",
    "build": "rimraf dist && tsc",
    "deploy": "gcloud app deploy",
    "clearPairs": "ts-node ./tools/clearPairs.ts",
    "getBalance": "ts-node ./tools/getBalance.ts",
    "closeCcPosition": "ts-node ./tools/closeCcPosition.ts",
    "closeBfPosition": "ts-node ./tools/closeBfPosition.ts",
    "closeQuPosition": "ts-node ./tools/closeQuPosition.ts",
    "closeAll": "npm run closeCcPosition && npm run closeBfPosition && npm run closeQuPosition && npm run clearPairs && npm run getBalance"
  },
  "dependencies": {
    "@bitr/awaitable-event-emitter": "^1.0.3",
    "@bitr/bitbankcc": "^1.0.5",
    "@bitr/bitflyer-fx": "^1.0.4",
    "@bitr/btcbox": "^1.0.3",
    "@bitr/castable": "^1.0.3",
    "@bitr/chronodb": "^1.0.2",
    "@bitr/logger": "^1.0.1",
    "@bitr/zmq": "^1.1.0",
    "chalk": "^4.0.0",
    "core-decorators": "^0.20.0",
    "date-fns": "^2.11.1",
    "decimal.js": "^10.2.0",
    "express": "^4.16.2",
    "fast-json-parse": "^1.0.3",
    "i18next": "^19.3.4",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "luxon": "^1.23.0",
    "mkdirp": "^1.0.3",
    "node-fetch": "^2.6.0",
    "opn": "^6.0.0",
    "pino": "^6.2.1",
    "reflect-metadata": "^0.1.13",
    "simple-statistics": "^7.0.8",
    "split2": "^3.1.1",
    "uuid": "^7.0.3",
    "ws": "^7.2.3",
    "zeromq": "^5.2.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/decimal.js": "^7.4.0",
    "@types/express": "^4.17.6",
    "@types/i18next": "^13.0.0",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/lodash": "^4.14.149",
    "@types/luxon": "^1.22.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^13.13.4",
    "@types/node-fetch": "^2.5.5",
    "@types/pino": "^5.17.0",
    "@types/split2": "^2.1.6",
    "@types/uuid": "^7.0.2",
    "@types/ws": "^7.2.3",
    "@types/zeromq": "^4.5.3",
    "@types/opn": "^5.1.0",
    "coveralls": "^3.0.11",
    "cpy-cli": "^3.1.0",
    "jest": "^25.5.0",
    "nock": "^12.0.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.2",
    "typescript": "3.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.test\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!src/index.ts",
      "!src/transport/index.ts",
      "!src/analytics/index.ts",
      "!src/container.config.ts",
      "!**/__tests__/**",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!tools/**",
      "!webui/**"
    ]
  }
}
